		"MFSH"
	],
	"cleanDirs": [
		{
			"path": "..\\build\\input\\pagecontent",
			"filter": "StructureDefinition-*-intro.xml"
		},
		{
			"path": "fsh",
			"filter": "*.fsh"
		},
		{
			"path": "..\\GraphFiles",
			"filter": "*.nodeGraph"
		}
	],
	"defines": [
		{
			"name": "%GraphDir%",
			"value": "..\\GraphFiles"
		},
		{
